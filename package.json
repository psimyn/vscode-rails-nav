{
    "name": "rails-nav",
    "displayName": "Rails Nav",
    "description": "Ruby on Rails related file navigation",
    "version": "0.0.1",
    "publisher": "hjleochen",
    "engines": {
        "vscode": "^1.0.0"
    },
    "repository": {
        "type": "git",
        "url": "https://github.com/hjleochen/vscode-rails-nav.git"
    },
    "homepage": "https://github.com/hjleochen/vscode-rails-nav",
    "categories": [
        "Other"
    ],
    "keywords": [
        "Ruby on Rails"
    ],
    "activationEvents": [
        "onCommand:extension.rails-nav"
    ],
    "main": "./out/src/extension",
    "contributes": {
        "commands": [
            {
                "command": "extension.rails-nav",
                "title": "Rails Nav"
            }
        ],
        "keybindings": [
            {
                "command": "extension.rails-nav",
                "key": "Alt+."
            }
        ]
    },
    "scripts": {
        "vscode:prepublish": "node ./node_modules/vscode/bin/compile",
        "compile": "node ./node_modules/vscode/bin/compile -watch -p ./",
        "postinstall": "node ./node_modules/vscode/bin/install"
    },
    "devDependencies": {
        "typescript": "^3.1.2",
        "vscode": "^1.1.21"
    },
    "dependencies": {
        "inflection": "^1.10.0"
    }
}
